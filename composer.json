{
    "name": "sandwave-io/dns-made-easy",
    "description": "PHP SDK for the DNS Made Easy API",
    "type": "library",
    "require": {
        "ext-json": "*",
        "php": "^8.1",
        "ext-mbstring": "*",
        "psr/container": "^2.0.2",
        "guzzlehttp/psr7": "^2.4.4",
        "psr/log": "^3.0.0",
        "spatie/enum": "^3.13.0",
        "guzzlehttp/guzzle": "^7.5.0",
        "illuminate/pagination": "^10.3.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.14.4",
        "php-http/mock-client": "^1.5.0",
        "phpstan/phpstan": "^1.10.6",
        "phpunit/phpunit": "^10.0.15",
        "monolog/monolog": "^3.3.1",
        "symfony/var-dumper": "^6.2.7",
        "phpstan/phpstan-strict-rules": "^1.5.0",
        "phpstan/phpstan-phpunit": "^1.3.10",
        "phpstan/phpstan-deprecation-rules": "^1.1.2",
        "sandwave-io/php-cs-fixer-config": "^1.0.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist"
    },
    "autoload": {
        "psr-4": {
            "DnsMadeEasy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DnsMadeEasy\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "test": "phpunit"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Jessica Smith",
            "email": "jsmith@tiggee.com"
        }
    ],
    "minimum-stability": "stable"
}
