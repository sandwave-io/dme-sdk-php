{
    "name": "sandwave-io/dns-made-easy",
    "description": "PHP SDK for the DNS Made Easy API",
    "type": "library",
    "require": {
        "ext-json": "*",
        "php": "^7.4|^8.0",
        "ext-mbstring": "*",
        "psr/container": "^1.0",
        "psr/http-client-implementation": "^1.0",
        "guzzlehttp/psr7": "^1.4.2|^1.7",
        "psr/log": "^1.1",
        "spatie/enum": "^3.6",
        "guzzlehttp/guzzle": "^6.2.0|^7.0",
        "illuminate/pagination": "^5.8|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "php-http/mock-client": "^1.4",
        "phpstan/phpstan": "^0.12.81",
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.5",
        "monolog/monolog": "^2.1",
        "symfony/var-dumper": "^5.1",
        "phpstan/phpstan-strict-rules": "^0.12.9",
        "phpstan/phpstan-phpunit": "^0.12.18",
        "phpstan/phpstan-deprecation-rules": "^0.12.6",
        "uptimeproject/php-cs-fixer-config": "^1.0.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist"
    },
    "autoload": {
        "psr-4": {
            "DnsMadeEasy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DnsMadeEasy\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "test": "phpunit"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Jessica Smith",
            "email": "jsmith@tiggee.com"
        }
    ],
    "minimum-stability": "stable"
}
